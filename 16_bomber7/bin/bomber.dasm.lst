    422  f236				   .PrepareScoreLoop		; this will loop twice, first X=1, and then X=0
    423  f236
    424  f236		       b5 84		      lda	Score,X	; load A with Timer (X=1) or Score (X=0)
    425  f238		       29 0f		      and	#$0F	; remove the tens digit by masking 4 bits 00001111
    426  f23a		       85 86		      sta	Temp	; save the value of A into Temp
    427  f23c		       0a		      asl		; shift left (it is now N*2)
    428  f23d		       0a		      asl		; shift left (it is now N*4)
    429  f23e		       65 86		      adc	Temp	; add the value saved in Temp (+N)
    430  f240		       95 87		      sta	OnesDigitOffset,X	; save A in OnesDigitOffset+1 or OnesDigitOffset
    431  f242
    432  f242		       b5 84		      lda	Score,X	; load A with Timer (X=1) or Score (X=0)
    433  f244		       29 f0		      and	#$F0	; remove the ones digit by masking 4 bits 11110000
    434  f246		       4a		      lsr		; shift right (it is now N/2)
    435  f247		       4a		      lsr		; shift right (it is now N/4)
    436  f248		       85 86		      sta	Temp	; save the value of A into Temp
