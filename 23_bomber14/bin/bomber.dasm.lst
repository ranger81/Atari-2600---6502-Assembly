    520  f2b6		       85 88		      sta	Temp	; save the value of A into Temp
    521  f2b8		       0a		      asl		; shift left (it is now N*2)
    522  f2b9		       0a		      asl		; shift left (it is now N*4)
    523  f2ba		       65 88		      adc	Temp	; add the value saved in Temp (+N)
