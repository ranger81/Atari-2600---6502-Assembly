    476  f28a		       29 f0		      and	#$F0	; remove the ones digit by masking 4 bits 11110000
    477  f28c		       4a		      lsr		; shift right (it is now N/2)
    478  f28d		       4a		      lsr		; shift right (it is now N/4)
