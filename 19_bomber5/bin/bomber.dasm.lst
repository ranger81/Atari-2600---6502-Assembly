    469  f280		       85 86		      sta	Temp	; save the value of A into Temp
    470  f282		       0a		      asl		; shift left (it is now N*2)
    471  f283		       0a		      asl		; shift left (it is now N*4)
    472  f284		       65 86		      adc	Temp	; add the value saved in Temp (+N)
    473  f286		       95 87		      sta	OnesDigitOffset,X	; save A in OnesDigitOffset+1 or OnesDigitOffset
    474  f288
